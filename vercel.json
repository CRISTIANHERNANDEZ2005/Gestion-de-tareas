{
  "version": 2,
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.1"
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/test",
      "dest": "/api/test.php"
    },
    {
      "src": "/db_test",
      "dest": "/api/db_test.php"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/api",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "VERCEL": "true"
  }
}